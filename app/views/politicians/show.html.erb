<div class="page-header">
  <%= link_to politicians_path, class: 'btn btn-default' do %>
    All Politicians
  <% end %>
  <%= link_to edit_politician_path(@politician), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show politician</h1>
</div>

<dl class="dl-horizontal">
  <dt>Last name:</dt>
  <dd><%= @politician.last_name %></dd>

  <dt>First name:</dt>
  <dd><%= @politician.first_name %></dd>

  <dt>Middle name:</dt>
  <dd><%= @politician.middle_name %></dd>

  <dt>Nickname:</dt>
  <dd><%= @politician.nickname %></dd>

  <dt>Full name:</dt>
  <dd><%= @politician.full_name %></dd>

  <dt>Birthday:</dt>
  <dd><%= @politician.birthday %></dd>

  <dt>Chamber:</dt>
  <dd><%= @politician.chamber %></dd>

  <dt>State:</dt>
  <dd><%= @politician.state %></dd>

  <dt>Party:</dt>
  <dd><%= @politician.party %></dd>

  <dt>Url:</dt>
  <dd><%= @politician.url %></dd>

  <dt>Address:</dt>
  <dd><%= @politician.address %></dd>

  <dt>Phone:</dt>
  <dd><%= @politician.phone %></dd>

  <dt>Contact form:</dt>
  <dd><%= @politician.contact_form %></dd>

</dl>
